<Window x:Class="DynamicSinumerikWrapperUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button Click="OpenConsole" >Console</Button>
        <Button Click="CreateClick" >Create Dynamic Wrapper</Button>
        <Button Click="TestConnectionDynamicWrapperClick">Test Connection with Dynamic Wrapper</Button>
        <TextBlock x:Name="TextBlock"></TextBlock>

    </StackPanel>
</Window>
